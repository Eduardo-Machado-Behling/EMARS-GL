.text
main:
	# jal draw_germany
	jal draw_germany
	jal draw_color
	
	li $v0, 10
	syscall
	

draw_germany:
	li $s0, 0x10080000 
	li $t1, 0
	li $t2, 0
	li $t3, 100
	li $t4, 100
	li $t6, 0
	li $t7, 1
	la $t5, SPR_GERMANY
	
	li $t0, 1
	sh $t1, 4($s0)
	sh $t2, 6($s0)
	sh $t3, 8($s0)
	sh $t4, 10($s0)
	sh $t7, 12($s0)
	sh $t6, 14($s0)
	sw $t5, 16($s0)
	sw $t0, ($s0)
	
	jr $ra

draw_color1:
	li $s0, 0x10080000 
	li $t1, 300
	li $t2, 100
	li $t3, 100
	li $t4, 100
	li $t6, 0
	li $t7, 0
	li $t5, 0xaa0022ff
	
	li $t0, 1
	sh $t1, 4($s0)
	sh $t2, 6($s0)
	sh $t3, 8($s0)
	sh $t4, 10($s0)
	sh $t7, 12($s0)
	sh $t6, 14($s0)
	sw $t5, 16($s0)
	sw $t0, ($s0)
	
	jr $ra

draw_color:
	li $s0, 0x10080000 
	li $t1, 50
	li $t2, 100
	li $t3, 100
	li $t4, 100
	li $t6, 0
	li $t7, 0
	li $t5, 0x00ff22ff
	
	li $t0, 2
	sh $t1, 20($s0)
	sh $t2, 22($s0)
	sh $t3, 24($s0)
	sh $t4, 26($s0)
	sh $t7, 28($s0)
	sh $t6, 30($s0)
	sw $t5, 32($s0)
	sw $t0, ($s0)
	
	jr $ra
